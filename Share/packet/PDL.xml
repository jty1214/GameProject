<PDL Version="3">
	<Packet name="REQUEST_ENTER_CHANNEL" from="Client">
		<Int name="world"/>
		<Int name="channel"/>
	</Packet>

	<Packet name="RESPONSE_ENTER_CHANNEL" from="Server">
		<Bool name="valid"/>
		<Long name="enterChannelTime"/>
	</Packet>

	<Packet name="REQUEST_FRIEND_LIST" from="Client">
	</Packet>

	<Packet name="RESPONSE_FRIEND_LIST" from="Server">
		<List class="FRIEND_LIST" name="friendList">
			<String name="userName"/>
			<Long name="userServerId"/>
			<Int name="userGrade"/>
			<Int name="userSubGrade"/>
		</List>
	</Packet>
</PDL>